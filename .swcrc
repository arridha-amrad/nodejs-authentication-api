{
  "jsc": {
    "target": "es2022",
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "decoratorMetadata": true
    },
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "experimental": {
      "keepImportAssertions": true
    }
  },
  "module": {
    "type": "es6",
    "noInterop": false
  }
}
